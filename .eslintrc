{
  "extends": "eslint-config-airbnb",
  "env": {
    "mocha": true,
    "node": true
  },
  "globals": {
    "expect": true
  },
  "rules": {
    "padded-blocks": 0,
    "no-use-before-define": [2, "nofunc"],
    "no-unused-expressions": 0,
    "no-reserved-keys": 0,
    "space-infix-ops": 0
  }
}
